{
      "name": "billing_sample",
      "version": "1.0.0",
      "description": "billing_sample server",
      "main": "src/app.js",
      "scripts": {
            "startWithPm2": "pm2 start src/app.js --interpreter babel-node -e js,yaml",
            "start": "nodemon src/app.js --exec babel-node -e js,yaml",
            "stopPm2": "pm2 stop src/app.js",
            "test": "NODE_ENV=test  nyc --reporter=html --reporter=text mocha --compilers js:babel-core/register ./testconfig/connect.test.js ./src/**/*.test.js ./testconfig/drop.test.js --exit",
            "flow": "flow",
            "clean": "rm -rf dist"
      },
      "author": "Pradeep Yadav",
      "license": "ISC",
      "dependencies": {
            "aes256": "1.0.2",
            "bcrypt": "^5.0.0",
            "bluebird": "3.5.1",
            "body-parser": "1.18.2",
            "compression": "1.7.1",
            "connect-mongo": "2.0.0",
            "cookie-parser": "1.4.3",
            "cors": "2.8.4",
            "dotenv": "4.0.0",
            "express": "4.16.2",
            "express-easy-helper": "1.1.0",
            "express-fileupload": "0.3.0",
            "express-session": "1.15.6",
            "helmet": "3.9.0",
            "jsonwebtoken": "8.1.0",
            "method-override": "2.3.10",
            "moment": "^2.24.0",
            "mongoose": "4.12.5",
            "mongoose-beautiful-unique-validation": "7.0.0",
            "mongoose-error-helper": "0.0.2",
            "mongoose-timestamp": "0.6.0",
            "mongoose-validator": "2.0.2",
            "passport": "0.4.0",
            "passport-local": "1.0.0",
            "pm2": "2.7.2",
            "swagger-jsdoc": "1.9.7",
            "swagger-tools": "0.10.3"
      },
      "devDependencies": {
            "babel-cli": "6.26.0",
            "babel-core": "6.26.0",
            "babel-eslint": "8.0.1",
            "babel-plugin-transform-flow-comments": "6.22.0",
            "babel-plugin-transform-runtime": "6.23.0",
            "babel-preset-env": "1.6.0",
            "babel-preset-es2015": "6.24.1",
            "babel-preset-es2016": "6.24.1",
            "babel-preset-flow": "6.23.0",
            "babel-preset-stage-0": "6.24.1",
            "babel-preset-stage-2": "6.24.1",
            "chai": "4.1.2",
            "codecov": "3.0.0",
            "concurrently": "3.5.0",
            "coveralls": "3.0.0",
            "eslint": "4.8.0",
            "flow-bin": "0.59.0",
            "mocha": "4.0.1",
            "morgan": "1.9.0",
            "nodemon": "1.12.1",
            "nyc": "11.3.0",
            "run-sequence": "2.2.0",
            "supertest": "3.0.0"
      },
      "nyc": {
            "require": [
                  "babel-register"
            ],
            "include": [
                  "src/**/*.js"
            ],
            "exclude": [
                  "src/**/controller.js",
                  "src/**/*.test.js",
                  "src/**/hook.js",
                  "src/**/seeds.js",
                  "src/**/index.js",
                  "src/**/config"
            ],
            "reporter": [
                  "lcov",
                  "text-summary"
            ],
            "sourceMap": true,
            "instrument": true
      },
      "engines": {
            "node": "8.8.0",
            "npm": "5.5.1"
      }
}
